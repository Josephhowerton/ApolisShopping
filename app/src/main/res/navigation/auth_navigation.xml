<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/auth_navigation"
    app:startDestination="@id/navigation_sign_in">

    <fragment
        android:id="@+id/navigation_sign_in"
        android:label="SignInFragment"
        tools:layout="@layout/fragment_sign_in"
        android:name="com.josephhowerton.apolisshopping.view.fragment.SignInFragment"/>
    <fragment
        android:id="@+id/navigation_sign_out"
        android:label="SignUpFragment"
        tools:layout="@layout/fragment_sign_up"
        android:name="com.josephhowerton.apolisshopping.view.fragment.SignUpFragment"/>
</navigation>